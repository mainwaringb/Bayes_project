model{
    for(n in 1:N){
        ystar[n] = inprod(x[n,],beta[])
        logit(p[n]) = ystar[n]
        y[n] ~ dbern(p[n])
        
        #manually impute missing x's for only a single variable, x2 = fav_afd
        x[n, 2] ~ dnorm(mu[n], tau)
        mu[n] = gamma[1] + gamma[2] * x[n, 3]
    }
    
    for(j in 1:J){
        beta[j] ~ dnorm(0, 0.01)
    }

    gamma[1] ~ dnorm(0, 0.01)
    gamma[2] ~ dnorm(0, 0.01)
    tau ~ dgamma(0.001, 0.001)
}
